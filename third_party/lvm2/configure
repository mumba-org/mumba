done

		check_lib_no_libs cman cman_init
		if test $NOTFOUND = 0; then
			{ $as_echo "$as_me:${as_lineno-$LINENO}: result: no pkg for libcman, using -lcman" >&5
$as_echo "no pkg for libcman, using -lcman" >&6; }
