mojom("mojo_bindings") {
  # This interface is internal to core.
  visibility = [ 
    "//core/*" 
  ]

  # indexed_db.mojom uses a native typemap that is not available in Java.
  cpp_only = true

  # imports vs deps check is disabled for this target to work around
  # windows multiple definitions linker error caused by having
  # both a direct and an indirect dependency on the same target
  #skip_deps_check = true

  sources = [
    "associated_interfaces.mojom",
    "child.mojom",
    "child_control.mojom",
    "child_memory_coordinator.mojom",
    "memory_coordinator.mojom",
    "leveldb_wrapper.mojom",
    "resource_usage_reporter.mojom",
    "window_shell_type.mojom",
    "fullscreen_video_element.mojom",
    #"load_timing_info.mojom"
    "service_names.mojom",
  ]

  if (is_win) {
    sources += [ "dwrite_font_proxy.mojom" ]
    sources += [ "font_cache_win.mojom" ]
  }

  if (is_mac) {
    sources += [ "font_loader_mac.mojom" ]
  }

  enabled_features = []
  if (enable_ipc_logging) {
    enabled_features += [ "ipc_logging" ]
  }
  if (is_linux || is_chromeos) {
    enabled_features += [ "supports_thread_priorities" ]
  }

  import_dirs = [ 
    "//lib/mojo/services",
    "//kit/cpp",
  ]

  public_deps = [
    ":mojom",
    #":service_names",
    "//lib/components/services/leveldb/public/interfaces",
    "//lib/ipc:mojom_constants",
    "//lib/media/mojo/interfaces",
    "//lib/mojo/public/mojom/base",
    "//lib/services/network/public/mojom",
    "//lib/services/service_manager/public/mojom",
    "//lib/services/ui/public/interfaces",
    "//lib/services/ui/public/interfaces/ime",
    "//lib/services/video_capture/public/mojom",
    "//lib/services/viz/public/interfaces",
    "//lib/services/tracing/public/mojom",
    "//lib/skia/public/interfaces",
    #"//third_party/blink/public:mojo_bindings",
    #"//third_party/blink/public:web_feature_mojo_bindings",
    "//third_party/blink/public/mojom:mojom_core",
    "//lib/ui/base/mojo:mojo_bindings",
    "//lib/ui/events/mojo:interfaces",
    "//lib/ui/gfx/geometry/mojo",
    "//lib/ui/gfx/mojo",
    "//lib/ui/gfx/range/mojo",
    "//lib/ui/latency/mojo:interfaces",
    "//lib/url/mojom:url_mojom_gurl",
    "//lib/url/mojom:url_mojom_origin",
  ]

  overridden_deps = [ "//third_party/blink/public/mojom:mojom_core" ]
