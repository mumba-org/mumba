mmap: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
openat: 1
mprotect: arg2 in ~PROT_EXEC || arg2 in ~PROT_WRITE
read: 1
close: 1
fstat: 1
newfstatat: 1
getpid: 1
sendto: 1
write: 1
stat: 1
gettid: 1
brk: 1
socket: arg0 == AF_VSOCK || arg0 == AF_UNIX
rt_sigaction: 1
rt_sigprocmask: 1
