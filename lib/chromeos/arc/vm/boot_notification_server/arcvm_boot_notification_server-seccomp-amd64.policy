listen: 1
access: 1
faccessat: 1
faccessat2: 1
arch_prctl: 1
munmap: 1
set_tid_address: 1
set_robust_list: 1
prlimit64: 1
ioctl: arg1 == TCGETS
connect: 1
lstat: 1
unlink: 1
chmod: 1
accept: 1
rt_sigtimedwait: 1
restart_syscall: 1
exit: 1
exit_group: 1
rt_sigreturn: 1
clock_gettime: 1
futex: 1
tgkill: 1
getpeername: 1
